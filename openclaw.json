{
  "name": "tomorrowdao-agent-skills",
  "description": "TomorrowDAO agent skill tools for DAO governance, network governance, BP election and resource trading. SECURITY: Never expose private keys in channel outputs.",
  "tools": [
    {
      "name": "tomorrowdao_dao_create",
      "description": "Create DAO on chain. Use execution mode simulate/send.",
      "command": "bun",
      "args": [
        "run",
        "tomorrowdao_skill.ts",
        "dao",
        "create"
      ],
      "cwd": ".",
      "inputSchema": {
        "type": "object",
        "properties": {
          "chainId": {
            "type": "string",
            "enum": [
              "AELF",
              "tDVV"
            ],
            "description": "Chain id"
          },
          "args": {
            "type": "object",
            "additionalProperties": true,
            "description": "Contract args object. Example: {\"metadata\":{\"name\":\"Demo DAO\"}}"
          },
          "mode": {
            "type": "string",
            "enum": [
              "simulate",
              "send"
            ],
            "default": "simulate",
            "description": "simulate: dry-run, send: broadcast tx"
          }
        },
        "required": [
          "args"
        ],
        "additionalProperties": true
      }
    },
